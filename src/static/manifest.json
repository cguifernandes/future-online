{
	"manifest_version": 3,
	"short_name": "Future Online",
	"name": "Future Online",
	"version": "1.0.0",
	"action": {
		"default_popup": "popup.html",
		"default_title": "Future Online",
		"default_icon": "icon.png"
	},
	"permissions": ["tabs", "storage", "activeTab"],
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"background": {
		"service_worker": "background.js"
	}
}
